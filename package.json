{
  "name": "is-angular-ivy-ready",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --prod && ng build --prod --app universal --output-hashing=false && npm run copy-all-functions",
    "copy-client-functions": "cp -R dist functions/",
    "copy-server-functions": "cp -R dist-server functions/",
    "copy-all-functions": "npm run copy-client-functions && npm run copy-server-functions",
    "firebase-serve": "firebase serve --only functions,hosting",
    "deploy": "firebase deploy",
    "test": "ng test",
    "test:no-sourcemaps": "ng test --sourcemaps=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --single-quote --trailing-comma es5 --write 'src/**/*.{ts,tsx,json,scss, css}'",
    "precommit": "lint-staged",
    "start-ssr": "tsc && node dist/out-tsc/server.js"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,scss, css}": [
      "npm run prettier",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/flex-layout": "5.0.0-beta.14",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.9",
    "@angular/router": "^5.2.0",
    "@nguniversal/express-engine": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "ramda": "^0.25.0",
    "rxjs": "^5.5.6",
    "ts-loader": "^3.5.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/express": "^4.11.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash.clonedeep": "^4.5.3",
    "@types/node": "~6.0.60",
    "@types/ramda": "^0.25.24",
    "codelyzer": "^4.0.1",
    "husky": "^0.14.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.0.4",
    "lodash.clonedeep": "^4.5.0",
    "prettier": "^1.11.1",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
